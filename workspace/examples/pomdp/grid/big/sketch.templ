			+ 1/98 : (o'=1) & (x'=9) & (y'=7)
			+ 1/98 : (o'=1) & (x'=9) & (y'=8)
			+ 1/98 : (o'=1) & (x'=9) & (y'=9);
			
	// move around the grid
	[east] o=1 & !(x=8 & y=0) & !(x=0 & y=1) -> (1-sl): (x'=min(x+1,9)) + sl: true; // not reached target
	[east] o=1 & x=8 & y=0 -> (1-sl): (x'=min(x+1,9)) & (o'=2) + sl: true; // reached target
	[east] o=1 & x=0 & y=1 -> (1-sl): (x'=1) & (o'=3) + sl: true; // reached bad state
	
	[west] o=1 & !(x=2 & y=1) -> (1-sl): (x'=max(x-1,0)) + sl: true; // not reached target
	[west] o=1 & x=2 & y=1 -> (1-sl): (x'=1) & (o'=3) + sl: true; // reached bad state

	[north] o=1 & !(x=1 & y=0)-> (1-sl): (y'=min(y+1,9)) + sl: true; // reached target
	[north]	o=1 & (x=1 & y=0) -> (1-sl): (y'=1) & (o'=3) + sl: true; //reached bad state
	
	[south] o=1 & !(x=9 & y=1) & !(x=1 & y=2) -> (1-sl): (y'=max(y-1,0)) + sl: true; // not reached target
	[south] o=1 & x=9 & y=1 -> (1-sl): (y'=max(y-1,0)) & (o'=2) + sl: true; // reached target
	[south] o=1 & x=1 & y=2 -> (1-sl): (y'=1) & (o'=3) + sl: true; //reached bad state	
	
	// reached target
	[done] o=2 -> true;
	
	//reached bad state
	[bad] o=3 -> true;
	
endmodule

// reward structure for number of steps to reach the target
rewards "steps"
        [east] true : 1;
        [west] true : 1;
        [north] true : 1;
        [south] true : 1;
endrewards

// target observation
label "goal" = o=2;
label "bad" = o=3;
